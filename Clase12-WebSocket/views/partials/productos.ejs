<h2 class="text-primary mt-4">Productos cargados</h2>
  <% if (content.length == 0) { %>
    <h5 class="alert-info rounded p-2">No se encontraron productos</h5>
    <% } else { %>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">TÃ­tulo</th>
          <th scope="col">Precio</th>
          <th scope="col">URL imagen</th>
        </tr>
      </thead>
      <tbody>
        <% content.forEach(producto => { %>
            <tr>
                <!-- Filas -->
                <th scope="row"><%= producto.id %></th>
                <td><%= producto.title %></td>
                <td><%= producto.price %></td>
                <td>
                  <img width="30" src="<%=producto.thumbnail%>" alt="">
                </td>
              <!-- Fin filas -->
            </tr>
        <% }); %> 
      </tbody>
    </table>
  <% } %>